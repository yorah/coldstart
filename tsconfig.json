{
    "compilerOptions": {
      "lib": [
        "ESNext"
      ],
      "removeComments": true,
      "moduleResolution": "node",
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "sourceMap": true,
      "target": "es2017",
      "outDir": "dist",
      "resolveJsonModule": true,
      "baseUrl": "./",
      "paths": {
        "@/*": [
          "./src/*"
        ]
      }
    },
    "include": [
      "./**/*.ts"
    ],
    "exclude": [
      "node_modules/**/*",
      ".serverless/**/*",
      ".webpack/**/*",
      "_warmup/**/*",
      ".vscode/**/*"
    ]
  }